alias rm="rm -i"
alias cp="cp -i"
alias mv="mv -i"
alias squidreload="service squid reload"
alias squidstart="service squid start"
alias squidstop="service squid stop"
alias squidstatus="service squid status"
alias squidconf="vim /etc/squid/squid.conf"
alias squidlog="tail -f /var/log/squid/access.log"
alias bashreload=". ~/.bashrc"
alias bashedit="vim ~/.bashrc"
